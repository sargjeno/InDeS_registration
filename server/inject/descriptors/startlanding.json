{
  "type": "object",
  "properties": {
    "isLanding": {
      "type": "identifier",
      "selector": "span",
      "control": "exists",
      "selectContains": "News"
    },
    "loggeduser": {
      "type": "string",
      "selector": ".k-link",
      "control": "property.innerText"
    }
  },
  "connect": [
    {
      "match": [
        "isLanding"
      ],
      "event": "clickTrainingBatchCreate"
    }
  ]
}
