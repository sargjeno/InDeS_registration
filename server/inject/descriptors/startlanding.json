{
  "type": "object",
  "properties": {
    "isLanding": {
      "type": "identifier",
      "selector": "html",
      "control": "url.trainingregistrationinbatch"
    },
    "loggeduser": {
      "type": "string",
      "selector": "#menu > li:nth-child(9) > span",
      "control": "property.innerText"
    }
  },
  "connect": [
    {
      "match": [
        "isLanding"
      ],
      "event": "clickTrainingBatchCreate"
    }
  ]
}
