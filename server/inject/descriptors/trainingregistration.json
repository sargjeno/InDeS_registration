{
  "type": "object",
  "comment": "Forms",
  "properties": {
    "isgroupregistartion": {
      "type": "identifier",
      "selector": ".title",
      "control": "exists",
      "selectContains": "Group administration"
    },
    "startdate": {
      "type": "date",
      "selector": "#trainingStartDate",
      "control": "text",
      "format": "YYYY-MM-DD"
    },
    "enddate": {
      "type": "date",
      "selector": "#trainingEndDate",
      "control": "text",
      "format": "YYYY-MM-DD"
    },
    "plannedcost": {
      "type": "string",
      "selector": ".k-formatted-value.k-input",
      "control": "text"
    },
    "realcost": {
      "type": "string",
      "selector": ".k-formatted-value.k-input",
      "control": "text"
    },
    "trainingtime": {
      "type": "string",
      "selector": ".k-formatted-value.k-input",
      "control": "text"
    },
    "quarter": {
      "type": "string",
      "selector": "#quarter",
      "control": "text"
    },
    "comment": {
      "type": "string",
      "selector": "#Comment",
      "control": "text"
    },
    "company": {
      "type": "string",
      "selector": "#trainingCompanyList",
      "control": "text",
      "selectHidden": true
    },
    "companyList": {
      "type": "array",
      "selectHidden": true,
      "selectorAll": "#trainingCompanyList_listbox > li",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "control": "property.innerText"
          },
          "click": {
            "type": "action",
            "control": "action.click"
          }
        }
      }
    },
    "trainingList": {
      "type": "array",
      "selectHidden": true,
      "selectorAll": "#trainingList_listbox > li",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "control": "property.innerText"
          },
          "click": {
            "type": "action",
            "control": "action.click"
          }
        }
      }
    },
    "trainerList": {
      "type": "array",
      "selectHidden": true,
      "selectorAll": "#trainerList_listbox > li",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "control": "property.innerText"
          },
          "click": {
            "type": "action",
            "control": "action.click"
          }
        }
      }
    },
    "employeeList": {
      "type": "array",
      "selectHidden": true,
      "selectorAll": "#employee_listbox > li",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "control": "property.innerText"
          },
          "click": {
            "type": "action",
            "control": "action.click"
          }
        }
      }
    },
    "selectedEmployees": {
      "type": "array",
      "selectorAll": "#employee_taglist > li",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "selector": "span",
            "control": "property.innerText"
          }
        }
      }
    },
    "realizationList": {
      "type": "array",
      "selectHidden": true,
      "selectorAll": "#realizationStatus_listbox > li",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "control": "property.innerText"
          },
          "click": {
            "type": "action",
            "control": "action.click"
          }
        }
      }
    },
    "statusList": {
      "type": "array",
      "selectHidden": true,
      "selectorAll": "#trainingStatus_listbox > li",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "control": "property.innerText"
          },
          "click": {
            "type": "action",
            "control": "action.click"
          }
        }
      }
    },
    "submit": {
      "type": "action",
      "selector": "button",
      "control": "action.button"
    },
    "realizationListDropdown": {
      "type": "object",
      "properties": {
        "selected": {
          "type": "object",
          "properties": {
            "label": {
              "type": "string",
              "control": "property.innerText"
            },
            "value": {
              "type": "string",
              "control": "property.innerText"
            }
          }
        },
        "options": {
          "type": "array",
          "selectHidden": true,
          "selectorAll": "#realizationStatus_listbox > li",
          "items": {
            "type": "object",
            "properties": {
              "label": {
                "type": "string",
                "control": "property.innerText"
              },
              "value": {
                "type": "string",
                "control": "property.innerText"
              }
            }
          }
        }
      }
    }
  },
  "connect": [
    {
      "match": [
        "isgroupregistartion"
      ],
      "screens": [
        "trainingregistration"
      ]
    }
  ],
  "selectHidden": true
}
