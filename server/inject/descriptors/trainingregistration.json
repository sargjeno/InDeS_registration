{
  "type": "object",
  "comment": "Forms",
  "properties": {
    "isgroupregistartion": {
      "type": "identifier",
      "selector": ".title",
      "control": "exists",
      "selectContains": "Group administration"
    },
    "startdate": {
      "type": "date",
      "selector": "#trainingStartDate",
      "control": "text",
      "format": "YYYY-MM-DD"
    },
    "enddate": {
      "type": "date",
      "selector": "#trainingEndDate",
      "control": "text",
      "format": "YYYY-MM-DD"
    },
    "plannedcost": {
      "type": "string",
      "selector": ".k-formatted-value.k-input",
      "control": "text"
    },
    "realcost": {
      "type": "string",
      "selector": ".k-formatted-value.k-input",
      "control": "text"
    },
    "trainingtime": {
      "type": "string",
      "selector": ".k-formatted-value.k-input",
      "control": "text"
    },
    "quarter": {
      "type": "string",
      "selector": "#quarter",
      "control": "text"
    },
    "comment": {
      "type": "string",
      "selector": "#Comment",
      "control": "text"
    },
    "traininglist": {
      "type": "select",
      "selector": "#trainingCompanyList_listbox > li",
      "control": "select"
    },
    "company": {
      "type": "string",
      "selector": "#trainingCompanyList",
      "control": "text",
      "selectHidden": true
    },
    "companyList": {
      "type": "array",
      "selectHidden": true,
      "selectorAll": "#trainingCompanyList_listbox > li",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "control": "property.innerText"
          },
          "click": {
            "type": "string",
            "control": "action.click"
          }
        }
      }
    }
  },
  "connect": []
}
